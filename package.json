{
  "name": "@senseyeinc/react-native-senseye-sdk",
  "version": "0.1.2",
  "description": "The Senseye SDK provides direct integration into the Senseye API on both iOS and Android for React Native apps. The JavaScript API is simple and cross-platform. Ready to get started? Follow the instructions below.",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-senseye-sdk.podspec",
    "!lib/typescript/example",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods",
    "docs": "live-server --open=./docs --port=9090"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "senseye"
  ],
  "repository": "https://github.com/senseye-inc/senseye-react-native-sdk",
  "author": "Senseye, Inc. <engineering@senseye.co> (https://senseye.co)",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/senseye-inc/senseye-react-native-sdk/issues"
  },
  "homepage": "https://github.com/senseye-inc/senseye-react-native-sdk#readme",
  "devDependencies": {
    "@react-native-community/eslint-config": "^2.0.0",
    "@react-native-picker/picker": "^1.9.10",
    "@release-it/conventional-changelog": "^1.1.4",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.13",
    "axios": "^0.21.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "live-server": "^1.2.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-builder-bob": "^0.17.1",
    "react-native-camera": "^3.40.0",
    "react-native-device-info": "^8.0.0",
    "release-it": "^13.5.8",
    "typedoc": "^0.20.14",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "@react-native-picker/picker": "*",
    "axios": "*",
    "react": "*",
    "react-native": "*",
    "react-native-camera": "*",
    "react-native-device-info": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "docs/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
